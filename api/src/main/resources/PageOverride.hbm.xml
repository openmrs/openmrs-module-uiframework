<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="org.openmrs.ui.framework.page.PageOverride" table="uiframework_page_override">
	
		<id name="pageOverrideId" type="java.lang.Integer" column="page_override_id">
			<generator class="native">
				<param name="sequence">uiframework_page_override_page_override_id_seq</param>
			</generator>
		</id>
		
		<discriminator column="page_override_id" insert="false" />

		<property name="uuid" type="string" length="38" unique="true" not-null="true" />

		<property name="providerName" type="string" column="provider_name" length="100" not-null="true"/>
		
		<property name="pageName" type="string" column="page_name" length="100" not-null="true"/>
				
		<property name="overriddenProviderName" type="string" column="overridden_provider_name" length="100" not-null="true"/>

		<property name="overriddenPageName" type="string" column="overridden_page_name" length="100" not-null="true"/>

		<property name="active" type="java.lang.Boolean" column="active" not-null="true"/>
		
	</class>
	
</hibernate-mapping>
